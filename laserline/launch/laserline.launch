<?xml version="1.0"?>
<launch>
  <node name="laserline" pkg="laserline" type="laserline">
    <param name="~frame_id" value="base_link" />
    <param name="~scan_topic" value="scan" />
    <param name="~frequency" value="10.0" />
    <param name="~show_lines" value="true" />
    <param name="~least_thresh" value="0.03" />
    <param name="~min_line_length" value="0.1" />
    <param name="~predict_distance" value="0.1" />
    <param name="~seed_line_points" value="3" />
    <param name="~min_line_points" value="4" />
  </node>
</launch>

