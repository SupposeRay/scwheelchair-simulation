<?xml version="1.0"?> 
<launch>

  <node name="obstacle_extractor" pkg="obstacle_detector" type="obstacle_extractor_node">
	<rosparam command="load" file="$(find obstacle_detector)/config/extractor_params.yaml" />
	<!--remap from="/pcl" to="/camera/depth/points"/-->
  </node>

   <node name="obstacle_tracker" pkg="obstacle_detector" type="obstacle_tracker_node"> 
	<rosparam command="load" file="$(find obstacle_detector)/config/tracker_params.yaml" /> 
    	<!--remap from="tracked_obstacles" to="obstacles"/-->
  </node> 

</launch>

